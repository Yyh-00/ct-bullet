{
  "name": "ct-bullet-ui",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix ./",
    "type:node": "tsc -p tsconfig.node.json --noEmit --composite false",
    "type:src": "rimraf ./dist && vue-tsc -p tsconfig.src.json --composite false --declaration --emitDeclarationOnly",
    "build:ui": "pnpm run type:src && pnpm --filter ./packages/** run build && rimraf ./dist",
    "dev:demo": "pnpm --filter @ct-bullet-ui/demo run dev",
    "dev:docs": "pnpm run build:ui && pnpm --filter @ct-bullet-ui/docs run dev",
    "build:docs": "pnpm --filter @ct-bullet-ui/docs run build",
    "docs:deploy": "cmd /c scripts\\deploy.bat"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.45.0",
    "eslint-define-config": "^1.21.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
  }
}
